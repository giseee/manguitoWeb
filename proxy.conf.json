{
  "/api": {
    "target": "http://localhost:4200",
    "secure": false,
    "changeOrigin": true,
    "headers": {
      "Access-Control-Allow-Origin": "*",
      "Access-Control-Allow-Methods": "GET,POST,PUT,DELETE",
      "Access-Control-Allow-Headers": "Content-Type, Access-Control-Allow-Headers"
    }
  }
}
